<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- сдесь следует перейти к тегу class -->
    <class name="com.geargames.regolith.units.tackle.StateTackle" table="state_tackle">
        <id name="id" type="int" column="id">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence_name">state_tackle_sequence</param>
            </generator>
        </id>
        <property name="state" column="state" type="short"/>
        <property name="firmness" column="firmness" type="short"/>
        <property name="upgrade" column="upgrade" type="byte"/>
    </class>
</hibernate-mapping>